# Criar o gr√°fico de barras interativo com Plotly Express
fig = px.bar(
    media_senioridade,
    x='senioridade',
    y='usd',
    title='M√©dia Salarial por Senioridade (USD)',
    labels={'senioridade': 'Senioridade', 'usd': 'M√©dia Salarial Anual (USD)'}
)

# Exibir o gr√°fico
fig.show()
